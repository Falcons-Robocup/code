#!/bin/bash
#
# Finalize a match, on coach.
# 
# JFEI 2015-07-19

# 


# construct list of robots to apply to
robots=`parse_robots $*`

# run per-robot commands
logdir=`newest_logdir.py`
team_apply_q.sh $robots match_finish_robot `hostname`:$logdir

# pack .bag and tracing
cd $logdir
tar czf matchlog.tgz *.bag *_[dp]trace*.txt stdout_A0_analyzer*.txt
echo "log file written:"
ls -altr1 $logdir/*.tgz

