#!/bin/bash
#
# Mapping of process name to the actual commands.
# NOTE: do NOT use tabs !!
# 


( 
cat << EOF
bh                  rosrun ballHandling ballHandling
commandGui          python /home/robocup/falcons/code/packages/coachCommands/commandGui.py
commW               rosrun rtdb3 commW
halCompass          rosrun peripheralsInterface compass
halIoBoard          rosrun peripheralsInterface ioBoard
halMotors           rosrun peripheralsInterface motors
heartbeat           rosrun heartBeat heartBeatNode
matchMode           rosrun heartBeat mainMatchMode
health              rosrun diagnostics robotHealth
logger              rosrun logging logger 30
rtdbSync            rosrun logging rtdbSync 30
pp                  /home/robocup/falcons/code/packages/pathPlanning/bin/pathplanningNode
mp                  /home/robocup/falcons/code/packages/motionPlanning/bin/motionPlanner
refboxGui           startRefbox
refboxRelay         rosrun refbox refboxRelay
robotControl        rosrun robotControl robotControlDaemon
roscore             roscore
simworld            rosrun simulation sw
sp                  /home/robocup/falcons/code/packages/shootPlanning/bin/shoot_planner
tp                  /home/robocup/falcons/code/packages/teamplay/bin/teamplay_main
vc                  rosrun velocityControl velocityControl
multiCam            /home/robocup/falcons/code/packages/multiCam/bin/multiCamNode
raspiSync           /home/robocup/falcons/code/packages/raspiSync/bin/raspiSync
wm                  /home/robocup/falcons/code/packages/worldModel/bin/worldModelNode
visualizer          rosrun visualizer visualizer
setpointGen         python /home/robocup/falcons/code/packages/pathPlanning/scripts/setpointGeneratorNode_2ndorder.py
EOF
) | sed 's/#.*//' # filter comments, like these ;)

